<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Scene12Controller">
   <top>
       <AnchorPane prefHeight="294.0" prefWidth="477.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="helloLabel" layoutY="14.0" prefHeight="37.0" prefWidth="477.0" text="查看店家清單">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Button fx:id="backB" layoutX="397.0" layoutY="246.0" mnemonicParsing="false" onAction="#toPreviousPage" text="Back" />
            <VBox layoutX="49.0" layoutY="70.0" prefHeight="154.0" prefWidth="397.0">
               <children>
                  <TableView fx:id="orderView" prefHeight="154.0" prefWidth="451.0">
                    <columns>
                        <TableColumn fx:id="idID" minWidth="1.0" prefWidth="33.0" text="id" />
                      <TableColumn fx:id="nameID" minWidth="1.0" prefWidth="100.0" text="name" />
                        <TableColumn fx:id="typeID" prefWidth="102.0" text="type" />
                      <TableColumn fx:id="addID" prefWidth="161.0" text="address" />
                    </columns>
                  </TableView>
               </children>
            </VBox>
            <Button fx:id="okB" layoutX="221.0" layoutY="246.0" mnemonicParsing="false" onAction="#selectRowFromTable" text="OK" />
         </children></AnchorPane>
   </top>
</BorderPane>
